@model CodeFirstBanking.Models.Checking

@{
    ViewData["Title"] = "Deposit";
}

<h2>Deposit</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

<div class="form-horizontal">
    <hr />

    <div class="form-group">
        <div class="col-md-10">
            @Html.LabelFor(model => model.CheckingId, "Checking Id:")
            @Html.EditorFor(Model => Model.CheckingId)
            
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-10">
            @Html.LabelFor(Model => Model.AccountBalance, "Deposit Amount:")
            @Html.EditorFor(Model => Model.AccountBalance)
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" />
        </div>
    </div>
</div>
    }
